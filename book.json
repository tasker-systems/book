{
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "title": "Tasker: Real-World Engineering Stories",
  "description": "Transform complex, fragile processes into reliable, observable workflows through compelling engineering stories.",
  "author": "Tasker Team",
  "language": "en",
  "direction": "ltr",
  "gitbook": ">=3.0.0",
  "plugins": [
    "github",
    "edit-link",
    "copy-code-button",
    "code",
    "prism",
    "anchorjs",
    "ga",
    "sitemap-general"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/tasker-systems/tasker"
    },
    "edit-link": {
      "base": "https://github.com/tasker-systems/tasker-blog/edit/main",
      "label": "Edit This Page"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-material-oceanic.css"
      ],
      "lang": {
        "ruby": "ruby",
        "bash": "bash",
        "yaml": "yaml",
        "json": "json",
        "curl": "bash"
      }
    },
    "ga": {
      "token": "UA-XXXXXXXX-X"
    },
    "sitemap-general": {
      "prefix": "https://tasker-engineering-stories.gitbook.io/"
    }
  },
  "variables": {
    "version": "2.6.0",
    "github_repo": "tasker-systems/tasker",
    "install_url": "https://raw.githubusercontent.com/tasker-systems/tasker/main/scripts/install-tasker-app.sh"
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  },
  "ignore": [
    ".cursor",
    "memory-bank",
    ".ruby-lsp",
    "_book",
    "node_modules",
    ".git",
    "*.log",
    "*.tmp",
    ".DS_Store"
  ]
}
